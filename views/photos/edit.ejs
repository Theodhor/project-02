<h1>EDIT PAGE</h1>
<h2>A populated form</h2>

<form  action="/photos/<%= photo._id %>" method="post">
  <input type="hidden" name="_method" value="put">
  <div>
    <label for="name">Name</label>
    <input id="name" name="name" placeholder="Name" value="<%= photo.name %>" required minlength="2"/>
  </div>

  <div>
    <label for="image">Name</label>
    <input id="image" name="image" placeholder="image" value="<%= photo.image %>" required pattern="^https?://.+" />
  </div>

  <button>Submit</button>
</form>
